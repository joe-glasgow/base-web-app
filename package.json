  "scripts": {
    "analyze": "npm build:with-stats && npm start:analyzer",
    "build": "rimraf ./dist; cross-env NODE_ENV=production TS_NODE_PROJECT=tsconfig.tooling.json ts-node scripts/build",
    "build:with-stats": "cross-env NODE_ENV=production TS_NODE_PROJECT=tsconfig.tooling.json webpack --config config/webpack.config.ts/client.prod.ts --json > build/client/static/bundle-stats.json",
    "clean:transpiled": "rimraf build/transpiled",
    "depgraph": "depcruise -c .dependency-cruiser.js --exclude '^node_modules' --output-type dot src | dot -T svg > dependency-graph.svg",
    "i18n:pull": "cross-env TS_NODE_PROJECT=tsconfig.tooling.json ts-node -r dotenv/config scripts/i18n-pull",
    "i18n:push": "cross-env TS_NODE_PROJECT=tsconfig.tooling.json ts-node -r dotenv/config scripts/i18n-push",
    "i18n:scan": "cross-env NODE_ENV=i18n npm transpile && npm i18next-scanner './build/transpiled/**/*.js' && npm clean:transpiled",
    "link-react": "npm link react react-dom",
    "lint": "concurrently npm:lint:js npm:lint:css npm:lint:deps",
    "lint:js": "eslint --ext .js,.jsx,.ts,.tsx src/ --fix",
    "lint:css": "stylelint src/**/*.css",
    "lint:deps": "depcruise -c .dependency-cruiser.js src/",
    "plop": "plop",
    "start:devserver": "cross-env NODE_ENV=development TS_NODE_PROJECT=tsconfig.tooling.json ts-node scripts/start",
    "start": "concurrently npm:start:devserver npm:storybook",
    "start:analyzer": "cross-env ANALYZE=true npm run build",
    "test": "cross-env TS_NODE_PROJECT=tsconfig.tooling.json ts-node scripts/test --env=jsdom",
    "test:update": "npm test --updateSnapshot",
    "transpile": "npm babel -d build/transpiled ./src --extensions .es6,.js,.es,.jsx,.mjs,.ts,.tsx --ignore **/*.d.ts",
    "tsc": "tsc --noEmit",
    "storybook": "start-storybook -p 8400",
    "build-storybook": "build-storybook"
  },
